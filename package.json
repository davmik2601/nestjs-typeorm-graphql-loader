{
  "name": "nestjs-graphql-typeorm-loader",
  "description": "A library to utilize the graphql-dataloader with nestjs and typeorm.",
  "version": "1.0.15",
  "author": "davmik2601",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davmik2601/nestjs-typeorm-graphql-loader.git"
  },
  "files": [
    "dist"
  ],
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "graphql",
    "typeorm",
    "dataloader",
    "loader"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepublish": "npm run build",
    "dev:start": "rimraf dist && tsc --watch",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "dependencies": {
    "apollo-server-plugin-base": "^3.6.3",
    "dataloader": "^2.2.2",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@apollo/gateway": "^2.1.4",
    "@nestjs/graphql": "^10.1.5",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/uuid": "^8.3.4",
    "apollo-server-fastify": "^3.10.3",
    "class-validator": "^0.14.0",
    "graphql": "^16.6.0",
    "mikro-orm": "^5.5.0",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.1.2",
    "typeorm": "^0.2.34"
  },
  "engines": {
    "node": ">= 16"
  }
}
